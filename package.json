{
  "name": "rangeslider-js",
  "version": "1.2.9",
  "main": "./src/index.js",
  "description": "lightweight rangeslider for browserify and the browser",
  "author": "Steffen BÃ¤r <>",
  "license": "MIT",
  "homepage": "https://github.com/stbaer/rangeslider-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stbaer/rangeslider-js.git"
  },
  "bugs": {
    "url": "https://github.com/stbaer/rangeslider-js/issues"
  },
  "keywords": [
    "browserify",
    "rangeslider",
    "plugin",
    "ui",
    "range",
    "slider"
  ],
  "scripts": {
    "uglify:dist": "uglifyjs ./dist/rangeslider-js.js --screw-ie8 -mc > ./dist/rangeslider-js.min.js",
    "uglify": "npm run uglify:dist && npm run show-gzipped-minjs-size",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && gulp build && npm run uglify",
    "show-gzipped-minjs-size": "echo \"js minified and gzipped: \" && gzip -c dist/rangeslider-js.min.js | wc -c",
    "example": "beefy --index=examples/standalone/example.html",
    "disc": "./node_modules/.bin/browserify --full-paths ./src/index.js > ./bundle.js && discify ./bundle.js > ./disc.html && rm ./bundle.js",
    "test-visual": "beefy test/visual/test.js --index=test/visual/beefy.html"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "custom-event": "^1.0.0",
    "debounce": "^1.0.0",
    "ev-pos": "0.0.4",
    "is-finite": "^1.0.1"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "disc": "^1.3.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-csso": "^1.0.1",
    "gulp-filter": "^3.0.1",
    "gulp-jshint": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "latest",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "jshint-summary": "^0.4.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "uglify-js": "latest",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  }
}
