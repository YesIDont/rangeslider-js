{
  "name": "rangeslider-js",
  "version": "1.2.6",
  "main": "./src/index.js",
  "description": "lightweight rangeslider for browserify and the browser",
  "author": "Steffen BÃ¤r <>",
  "license": "MIT",
  "homepage": "https://github.com/stbaer/rangeslider-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stbaer/rangeslider-js.git"
  },
  "bugs": {
    "url": "https://github.com/stbaer/rangeslider-js/issues"
  },
  "keywords": [
    "browserify",
    "rangeslider",
    "plugin",
    "ui",
    "range",
    "slider"
  ],
  "scripts": {
    "uglify:dist": "uglifyjs --screw-ie8 ./dist/rangeslider-js.js -mc > ./dist/rangeslider-js.min.js",
    "uglify": "npm run uglify:dist && npm run show-gzipped-minjs-size",
    "clean": "rm -r dist/*",
    "build": "npm run clean && gulp build && npm run uglify",
    "show-gzipped-minjs-size": "echo \"js minified and gzipped: \" && gzip -c dist/rangeslider-js.min.js | wc -c",
    "example": "beefy --index=examples/standalone/example.html",
    "docs": "./node_modules/.bin/jsdoc -c ./gulp/util/jsdoc.conf.json",
    "disc": "./node_modules/.bin/browserify --full-paths ./src/index.js > ./bundle.js && discify ./bundle.js > ./disc.html && rm ./bundle.js",
    "test-visual": "beefy test/visual/test.js --index=test/visual/beefy.html"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "ev-pos": "0.0.4"
  },
  "devDependencies": {
    "browser-sync": "^2.9.6",
    "browserify": "^11.2.0",
    "disc": "^1.3.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-cached": "^1.1.0",
    "gulp-csso": "^1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-jshint": "^1.11.2",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "latest",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "ink-docstrap": "^0.5.2",
    "jshint-summary": "^0.4.0",
    "lodash": "^3.10.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
    "uglify-js": "latest",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0"
  }
}
